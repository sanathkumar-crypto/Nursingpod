# This is an example file. Create actual secrets using:
# kubectl create secret generic nursingpod-secrets \
#   --from-literal=google-client-id='YOUR_CLIENT_ID' \
#   --from-literal=google-client-secret='YOUR_CLIENT_SECRET' \
#   --from-literal=secret-key='YOUR_SECRET_KEY'

# Or use Sealed Secrets or External Secrets Operator for production
apiVersion: v1
kind: Secret
metadata:
  name: nursingpod-secrets
type: Opaque
stringData:
  google-client-id: "YOUR_GOOGLE_CLIENT_ID_HERE"
  google-client-secret: "YOUR_GOOGLE_CLIENT_SECRET_HERE"
  secret-key: "YOUR_SECRET_KEY_HERE"




